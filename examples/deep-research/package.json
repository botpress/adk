{
  "name": "deep-research",
  "version": "1.0.0",
  "description": "A Botpress Agent built with the ADK",
  "type": "module",
  "packageManager": "bun@1.3.2",
  "scripts": {
    "dev": "adk dev & bun --bun --port 3002 src/ui/index.html",
    "dev:ui": "bun --bun --port 3002 src/ui/index.html",
    "build": "adk build && bun run bun.build.ts",
    "build:ui": "bun run bun.build.ts",
    "deploy": "adk deploy",
    "postinstall": "rm -rf node_modules/@botpress/webchat/node_modules/react node_modules/@botpress/webchat/node_modules/react-dom"
  },
  "dependencies": {
    "@botpress/runtime": "^1.7.5",
    "@botpress/webchat": "^4.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
